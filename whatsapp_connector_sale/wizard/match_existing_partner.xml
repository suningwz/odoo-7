<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_match_existing_partner_wizard" model="ir.ui.view">
        <field name="name">match.existing.partner.wizard</field>
        <field name="model">match.existing.partner.wizard</field>
        <field name="arch" type="xml">
            <form >
                <group>
                    <field name="conversation_id" invisible="1" />
                    <field name="res_partner_id" options="{'no_open': True, 'no_create': True}" />
                </group>
                <footer>
                    <button name="match_partner" type="object" string="Accept" class="oe_highlight" />
                    <button string="Close" class="oe_highlight" special="cancel"/>
                </footer>
           </form>
        </field>
    </record>

</odoo>
